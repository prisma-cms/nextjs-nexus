{
  "name": "@prisma-cms/nextjs",
  "author": "@Fi1osof",
  "license": "MIT",
  "version": "1.6.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.server.json ts-node-dev --trace-deprecation --ignore-watch=/.next/ --rs --no-notify --respawn --transpile-only server",
    "build": "next build && yarn build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:analyze": "cross-env ANALYZE=true yarn build",
    "generate:types": "cross-env NODE_PATH=. ts-node -P tsconfig.server.json src/modules/gql/cli/generateTypes/run",
    "types": "tsc --pretty --noEmit",
    "types:server": "yarn types -P tsconfig.server.json",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "prisma:format": "yarn prisma format",
    "prisma:db:push": "cross-env TS_NODE_PROJECT=tsconfig.server.json yarn prisma db push --preview-feature",
    "prisma:migrate:status": "cross-env TS_NODE_PROJECT=tsconfig.server.json yarn prisma migrate status --preview-feature",
    "prisma:migrate:create": "cross-env TS_NODE_PROJECT=tsconfig.server.json yarn prisma migrate dev --preview-feature",
    "prisma:deploy": "yarn prisma migrate deploy --preview-feature",
    "prisma:seed": "cross-env TS_NODE_PROJECT=tsconfig.server.json yarn prisma db seed --preview-feature",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env TS_NODE_PROJECT=tsconfig.server.json ts-node --transpile-only server/nexus",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus && npm -s run generate:types",
    "clean": "rimraf ./dist && rimraf ./.next",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "test:api": "cross-env TEST_REAL_API=true jest",
    "test:all": "yarn lint && yarn types && yarn cy:run && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run types"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
