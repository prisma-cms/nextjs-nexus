{
  "name": "@prisma-cms/nextjs",
  "author": "@Fi1osof",
  "license": "MIT",
  "version": "1.1.1",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "generate:types": "ts-node -P tsconfig.server.json src/modules/gql/cli/generateTypes/run",
    "types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn types && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run types"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "cross-fetch": "^3.0.6",
    "graphql": "^15.3.0",
    "http-proxy-middleware": "^1.0.5",
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/near-operation-file-preset": "^1.17.11",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@next/bundle-analyzer": "^9.5.5",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.161",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/urijs": "^1.19.12",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3"
  },
  "description": "Bootstrap a developer-friendly NextJS app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma-cms/nextjs.git"
  },
  "keywords": [
    "prisma-cms",
    "next-js",
    "typescript",
    "ts-node",
    "graphql",
    "headless-cms"
  ],
  "bugs": {
    "url": "https://github.com/prisma-cms/nextjs/issues"
  },
  "homepage": "https://github.com/prisma-cms/nextjs#readme"
}
